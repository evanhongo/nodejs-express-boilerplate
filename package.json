    "@types/jest": "^27.5.2",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/morgan": "^1.9.4",
    "@types/node": "^8.10.66",
    "@types/passport": "^1.0.11",
    "@types/passport-facebook": "^2.1.11",
    "@types/passport-jwt": "^3.0.8",
    "@typescript-eslint/parser": "^5.49.0",
