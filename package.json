    "nodemon": "^2.0.20",
    "prettier": "^2.8.3",
    "prettier-eslint": "^15.0.1",
    "prettier-eslint-cli": "^7.1.0",
    "ts-jest": "^29.0.5",
    "ts-loader": "9.4.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.2",
    "tsconfig-paths-webpack-plugin": "^4.0.0",
    "typescript": "^4.9.4",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-node-externals": "^3.0.0"
  },
  "dependencies": {
    "@apollo/server": "^4.3.2",
    "@typegoose/typegoose": "^10.0.0",
    "class-validator": "^0.14.0",
    "cls-rtracer": "^2.6.2",
    "compression": "^1.7.4",
    "connect-redis": "^6.1.3",
    "cookie-parser": "^1.4.6",
    "dataloader": "^2.1.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-rate-limit": "^5.5.1",
    "express-session": "^1.17.3",
    "graphql": "^16.6.0",
    "graphql-upload": "^13.0.0",
    "helmet": "^4.6.0",
    "inversify": "^6.0.1",
    "jsonwebtoken": "^9.0.0",
    "logform": "^2.4.2",
    "mongodb": "^4.13.0",
    "mongoose": "^6.9.0",
    "morgan": "^1.10.0",
    "passport": "^0.6.0",
    "passport-facebook": "^3.0.0",
    "passport-jwt": "^4.0.1",
    "passport-line-auth": "^0.2.9",
    "redis": "^4.6.1",
    "reflect-metadata": "^0.1.13",
    "sift": "^13.5.4",
    "type-graphql": "2.0.0-beta.1",
    "winston": "^3.8.2",
    "zod": "^3.20.2"
  },
  "scripts": {
    "dev": "ts-node -r tsconfig-paths/register ./src/main.ts",
    "watch": "nodemon --watch src --delay 0.5 --ext ts --exec pnpm run dev",
    "build-cjs": "webpack --config webpack.config.js",
    "build-binary": "webpack --config webpack.config.js && pkg -c package.json --compress GZip -o ./build/app build/main.js",
    "start": "pm2 start ./ecosystem.config.js",
    "test": "jest",
    "lint": "prettier-eslint --write src/**/*.{ts,tsx}"
  },
  "pkg": {
    "assets": [
      "node_modules/@apollo/server/**",
      "node_modules/@typegoose/typegoose/**",
      "node_modules/class-validator/**",
      "node_modules/cls-rtracer/**",
      "node_modules/compression/**",
      "node_modules/connect-redis/**",
      "node_modules/cookie-parser/**",
      "node_modules/dataloader/**",
      "node_modules/dotenv/**",
      "node_modules/express/**",
      "node_modules/express-rate-limit/**",
      "node_modules/express-session/**",
      "node_modules/graphql/**",
      "node_modules/graphql-upload/**",
      "node_modules/helmet/**",
      "node_modules/inversify/**",
      "node_modules/jsonwebtoken/**",
      "node_modules/logform/**",
      "node_modules/mongodb/**",
      "node_modules/mongoose/**",
      "node_modules/morgan/**",
      "node_modules/passport/**",
      "node_modules/passport-facebook/**",
      "node_modules/passport-jwt/**",
      "node_modules/passport-line-auth/**",
      "node_modules/redis/**",
      "node_modules/reflect-metadata/**",
      "node_modules/sift/**",
      "node_modules/type-graphql/**",
      "node_modules/winston/**",
      "node_modules/zod/**"
    ],
    "targets": [
      "node18-win-x64",
      "node18-linux-x64"
    ]
  }
}